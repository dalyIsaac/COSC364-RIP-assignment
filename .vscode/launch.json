  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Run router",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/router.py",
      "console": "integratedTerminal",
      "args": ["./Config_files/R2a.cfg"]
    },
    {
      "name": "Python: Run router in external terminal",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/router.py",
      "console": "externalTerminal",
      "args": ["./Config_files/R2a.cfg"]
    },
    {
      "name": "Python: Current File (Integrated Terminal)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    }
  ]
}
